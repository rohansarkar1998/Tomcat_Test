	  <from-view-id>Login.jsp</from-view-id> 
	  <navigation-case> <from-outcome>Adminhome</from-outcome>
	   <to-view-id>Adminhome.jsp</to-view-id> 
	   </navigation-case>
	    </navigation-rule>
	    <navigation-rule>
	 <display-name>Showdoc</display-name>
	  <from-view-id>Showdoc.jsp</from-view-id> 
	  <navigation-case> <from-outcome>Showdoc.jsp</from-outcome>
	   <to-view-id>Showdoc.jsp</to-view-id> 
	   </navigation-case>
	    </navigation-rule>
	     <navigation-rule>
	 <display-name>PatientUpdate</display-name>
	  <from-view-id>PatientUpdate.jsp</from-view-id> 
	  <navigation-case> <from-outcome>SuccessUpdate</from-outcome>
	   <to-view-id>SuccessUpdate.jsp</to-view-id> 
