	    </navigation-rule>
	    <navigation-rule>
	 <display-name>Add</display-name>
	  <from-view-id>Add.jsp</from-view-id> 
	  <navigation-case> <from-outcome>SuccessUpdate</from-outcome>
	   <to-view-id>SuccessUpdate.jsp</to-view-id> 
	   </navigation-case>
	    </navigation-rule>
	     <navigation-rule>
	 <display-name>ChangePass</display-name>
	  <from-view-id>ChangePass.jsp</from-view-id> 
	  <navigation-case> <from-outcome>SuccessUpdate</from-outcome>
	   <to-view-id>SuccessUpdate.jsp</to-view-id> 
	   </navigation-case>
	    </navigation-rule>
	    

</faces-config>
